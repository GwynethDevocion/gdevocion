function getProductAttributeValue (attributeName, attributeType) {
	var attributeValue = '';
	var attributeValueArray = [];
	var delimiter = ',';
	if (typeof digitalData.product != 'undefined') {
	for(i=0;i<digitalData.product.length;i++){
			var productObj = digitalData.product[i];
			switch (attributeType) {
			case 'category':
				attributeValue = typeof productObj.category != 'undefined' && productObj.category[attributeName] != 'undefined' ? productObj.category[attributeName] : '';
				break;
			case 'pCat':
				attributeValue = typeof productObj.pCat != 'undefined' && typeof productObj.pCat[attributeName] != 'undefined'? digitalData.product[i].pCat[attributeName] : '';
				break;
			case 'attribute':
				attributeValue = typeof productObj.attributes != 'undefined' && typeof productObj.attributes[attributeName] != 'undefined' ? productObj.attributes[attributeName] : '';
				break;
			case 'rating':
				attributeValue = typeof productObj[attributeName] != 'undefined' ? productObj[attributeName] : '';
				break;
			default:
				attributeValue = typeof productObj.productInfo != 'undefined' && typeof productObj.productInfo[attributeName] != 'undefined' ? productObj.productInfo[attributeName] : '';
				break;
			}

		}
		attributeValueArray.push(attributeValue);
	}
	return attributeValueArray.join(delimiter);
}

